if [ "$JNA_JAR" != "" ]; then
    JNA_JARS="${CLASSPATHSEPARATOR}${JNA_JAR}"
    
fi

#### JMF - Java Native Access jar file (jna.jar).  See
# https://jna.dev.java.net/.  Used in ptolemy/actor/lib/opencv
JMF_JARS=@JMF_JARS@
if [ "$JMF_JARS" != "" ]; then
    JMF_JARS="${CLASSPATHSEPARATOR}${JMF_JARS}"
    
fi

#### Jode..  Java decompiler
# The jode jar file.
# JODE_JAR might contain ${JODE_DIR)
JODE_DIR="@JODE_DIR@"
JODE_JAR="@JODE_CLASSES@"

