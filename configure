	    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Failed to compile config/gmpTest.c,
    \"$PTCC\" gmpTest.c $PTGMP_INCLUDE -o gmpTest $PTGMP_LIB -lgmp
    $RESULTS" >&5
