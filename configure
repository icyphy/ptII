	   if test  -f "$PTOAUTHCOMMON_JAR"; then
	      if test "${PTVERBOSE:-none}" = "none"; then { $as_echo "$as_me:${as_lineno-$LINENO}: result: ok" >&5
$as_echo "ok" >&6; }; else { $as_echo "$as_me:${as_lineno-$LINENO}: result: $PTOAUTHCOMMON_JAR" >&5
