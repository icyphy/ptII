	    echo $ac_n "checking for jjtree""... $ac_c" 1>&6
echo "configure:2744: checking for jjtree" >&5
	    if test -x "$JAVACC_DIR/bin/jjtree"; then
		# Note that we don't substitute in the $(JAVACC_DIR) here
