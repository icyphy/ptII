            <property name="input"/>
        </port>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
    </entity>
    <entity name="PeriodicTrigger" class="ptolemy.domains.rtp.lib.PeriodicTrigger">
        <property name="frequency" class="ptolemy.data.expr.Parameter" value="2.0">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="120.0, 320.0">
        </property>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
    </entity>
    <entity name="Clock" class="ptolemy.actor.lib.Clock">
        <property name="stopTime" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <property name="period" class="ptolemy.data.expr.Parameter" value="2.0">
        </property>
        <property name="offsets" class="ptolemy.data.expr.Parameter" value="{0.0, 1.0}">
        </property>
        <property name="values" class="ptolemy.data.expr.Parameter" value="{1, 0}">
        </property>
