PTSERVER_USER_DIR2="$PTII/ptolemy/homer"
if test ! -d "$PTSERVER_USER_DIR" -o ! -d "$PTSERVER_USER_DIR2" -o -z "$PTHOMER_DIR"; then
    if test "${PTVERBOSE:-none}" != "none"; then { $as_echo "$as_me:${as_lineno-$LINENO}: result: $PTSERVER_USER_DIR or
    $PTSERVER_USER_DIR2 are not present or
    Homer was not found
    so we need not find the Hessian, Jetty, Servlet and WMQTT interfaces for ptserver." >&5
