    <entity name="Dog" class="org.terraswarm.accessor.jjs.JSAccessor">
        <property name="script" class="ptolemy.actor.parameters.PortParameter" value="// Declare audio clip globally so it may be started and stopped in separate invocations&#10;&#10;var audio = new javafx.scene.media.AudioClip(&quot;http://c0026106.cdn1.cloudfiles.rackspacecloud.com/2fbc94686ceb434a864c0691906bd144_dog.mp3&quot;);&#10;&#10;function fire() {&#10;   // Attempt to get tokens from both input ports&#10;   var triggerValue = get(trigger);&#10;   var stopValue = get(stop);&#10;   &#10;   // If stop port has a token, stop playback, regardless of whether or not trigger has a token&#10;   // This is consistent with ClipPlayer&#10;&#10;   if (stopValue != null) {&#10;      audio.stop();  &#10;      send(false, output);&#10;   } else if (triggerValue != null) {&#10;      audio.play();&#10;      send(true, output);&#10;   }&#10;}&#10;">
        </property>
