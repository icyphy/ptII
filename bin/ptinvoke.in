    # used by bin/ptinvoke.in
    CALTROP_DIR="@CALTROP_DIR@"
    CALTROP_JARS="${CLASSPATHSEPARATOR}${CALTROP_DIR}/ptCal.jar${CLASSPATHSEPARATOR}${CALTROP_DIR}/java_cup.jar"
