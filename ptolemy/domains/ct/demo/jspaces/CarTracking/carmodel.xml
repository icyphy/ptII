<?xml version="1.0" standalone="no"?>
<!DOCTYPE class PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<class name="carmodel" extends="ptolemy.actor.TypedCompositeActor">
    <property name="_createdBy" class="ptolemy.kernel.util.VersionAttribute" value="2.0-beta">
    </property>
    <doc>CT model for the a car, without a director</doc>
    <port name="forceInput" class="ptolemy.actor.TypedIOPort">
        <property name="forceInput" class="ptolemy.kernel.util.Attribute">
        </property>
    </port>
    <port name="velocityOutput" class="ptolemy.actor.TypedIOPort">
        <property name="velocityOutput" class="ptolemy.kernel.util.Attribute">
        </property>
    </port>
    <port name="positionOutput" class="ptolemy.actor.TypedIOPort">
        <property name="positionOutput" class="ptolemy.kernel.util.Attribute">
        </property>
    </port>
    <entity name="add1" class="ptolemy.actor.lib.AddSubtract">
        <port name="plus" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="multiport"/>
        </port>
        <port name="minus" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="multiport"/>
        </port>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
    </entity>
    <entity name="scaleM" class="ptolemy.actor.lib.Scale">
        <property name="factor" class="ptolemy.data.expr.Parameter" value="1.0">
        </property>
        <property name="scaleOnLeft" class="ptolemy.data.expr.Parameter" value="true">
        </property>
        <property name="_icon" class="ptolemy.vergil.icon.AttributeValueIcon">
            <property name="attributeName" class="ptolemy.kernel.util.StringAttribute" value="factor">
            </property>
            <property name="displayWidth" class="ptolemy.data.expr.Parameter" value="6">
            </property>
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
        </port>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
    </entity>
    <entity name="scaleN" class="ptolemy.actor.lib.Scale">
        <property name="factor" class="ptolemy.data.expr.Parameter" value="0.5">
        </property>
        <property name="scaleOnLeft" class="ptolemy.data.expr.Parameter" value="true">
        </property>
        <property name="_icon" class="ptolemy.vergil.icon.AttributeValueIcon">
            <property name="attributeName" class="ptolemy.kernel.util.StringAttribute" value="factor">
            </property>
            <property name="displayWidth" class="ptolemy.data.expr.Parameter" value="6">
            </property>
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
        </port>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
    </entity>
    <entity name="velocity" class="ptolemy.domains.ct.lib.Integrator">
        <property name="initialState" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="signalType" class="ptolemy.data.expr.Parameter" value="&quot;CONTINUOUS&quot;">
            </property>
        </port>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
            <property name="signalType" class="ptolemy.data.expr.Parameter" value="&quot;CONTINUOUS&quot;">
            </property>
        </port>
    </entity>
    <entity name="position" class="ptolemy.domains.ct.lib.Integrator">
        <property name="initialState" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="signalType" class="ptolemy.data.expr.Parameter" value="&quot;CONTINUOUS&quot;">
            </property>
        </port>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
            <property name="signalType" class="ptolemy.data.expr.Parameter" value="&quot;CONTINUOUS&quot;">
            </property>
        </port>
    </entity>
    <relation name="r33" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="r44" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="r55" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="r66" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="r77" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="r88" class="ptolemy.actor.TypedIORelation">
    </relation>
    <link port="forceInput" relation="r33"/>
    <link port="velocityOutput" relation="r66"/>
    <link port="positionOutput" relation="r88"/>
    <link port="add1.plus" relation="r33"/>
    <link port="add1.minus" relation="r77"/>
    <link port="add1.output" relation="r44"/>
    <link port="scaleM.input" relation="r44"/>
    <link port="scaleM.output" relation="r55"/>
    <link port="scaleN.input" relation="r66"/>
    <link port="scaleN.output" relation="r77"/>
    <link port="velocity.input" relation="r55"/>
    <link port="velocity.output" relation="r66"/>
    <link port="position.input" relation="r66"/>
    <link port="position.output" relation="r88"/>
</class>
