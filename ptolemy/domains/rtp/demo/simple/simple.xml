    </property>
    <property name="RTP" class="ptolemy.domains.rtp.kernel.RTPDirector">
        <property name="executionDuration" class="ptolemy.data.expr.Parameter" value="10000">
        </property>
    </property>
    <entity name="Clock0" class="ptolemy.actor.lib.Clock">
        <property name="stopTime" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <property name="period" class="ptolemy.data.expr.Parameter" value="1000.0">
        </property>
        <property name="offsets" class="ptolemy.data.expr.Parameter" value="{250.0,750.0}">
        </property>
        <property name="values" class="ptolemy.data.expr.Parameter" value="{1,0}">
