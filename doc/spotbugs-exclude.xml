    <Match classregex="soot.*"/>
    <Match classregex="sun.net.*"/>
    <Match classregex="text.*"/>
    <Match classregex="tcl.*"/>
    <Match classregex="test.*"/>
    <Match classregex="util.ClassnameFilterList"/>
    <Match classregex="vendors.vr.*"/>
    <Match classregex="vm.*"/>
    <Match classregex="xjava.*"/>
    <Match classregex=".*">
       <!-- Exclude Serializable warnings -->
       <!-- Exclude class names shouldn't shadow simple name of super class
            because cg adapters shadow. -->
       <!-- Exclude class is Serializable; consider declaring a serialVersionUID -->
       <BugCode name="Nm,Se,SnVI" />
    </Match>
</FindBugsFilter>
