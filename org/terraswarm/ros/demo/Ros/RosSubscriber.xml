            <property name="server (port-parameter)" class="ptolemy.kernel.util.StringAttribute" value="The IP address or domain name of the server.">
            </property>
            <property name="port (port-parameter)" class="ptolemy.kernel.util.StringAttribute" value="The port that the web socket server listens to.">
            </property>
            <property name="topic (port-parameter)" class="ptolemy.kernel.util.StringAttribute" value="The topic to subscribe to.">
            </property>
            <property name="received (port)" class="ptolemy.kernel.util.StringAttribute" value="The message received from the web socket server.">
            </property>
        </property>
