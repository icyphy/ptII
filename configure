if eval "test \"`echo '$''{'ac_cv_path_JJTREE'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  case "$JJTREE" in
  /*)
  ac_cv_path_JJTREE="$JJTREE" # Let the user override the test with a path.
  ;;
  ?:/*)			 
  ac_cv_path_JJTREE="$JJTREE" # Let the user override the test with a dos path.
  ;;
  *)
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS=":"
  ac_dummy="$PATH"
  for ac_dir in $ac_dummy; do 
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      ac_cv_path_JJTREE="$ac_dir/$ac_word"
      break
    fi
  done
  IFS="$ac_save_ifs"
  ;;
