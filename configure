        cd "$startdir"
        fi
    fi
    if test "$RXTX_PRESENT" = "no"; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Perhaps rxtx is not installed?
    You need not download rxtx unless you plan the actors in actor.lib.comm.
