<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<entity name="GenericSources" class="ptolemy.moml.EntityLibrary">
  <configure>
    <?moml
      <group>
<doc>Sources of sequences of values. </doc>

<entity name="Const" class="ptolemy.actor.lib.Const">
<doc>Create a constant sequence.</doc>
   <property name="_icon" class="ptolemy.vergil.icon.BoxedValueIcon">
      <property name="attributeName" value="value"/>
      <property name="displayWidth" value="60"/>
   </property>
</entity>
<!-- The following actor doesn't work as advertised.
<entity name="ConstPlaceable" class="ptolemy.actor.lib.gui.ConstPlaceable">
<doc>Create a constant sequence that is visible in a GUI.</doc>
   <property name="_icon" class="ptolemy.vergil.icon.BoxedValueIcon">
      <property name="attributeName" value="value"/>
      <property name="displayWidth" value="60"/>
   </property>
</entity>
-->
<entity name="StringConst" class="ptolemy.actor.lib.StringConst">
<doc>Create a constant sequence with a string value.</doc>
   <property name="_icon" class="ptolemy.vergil.icon.BoxedValueIcon">
      <property name="attributeName" value="value"/>
      <property name="displayWidth" value="80"/>
      <property name="displayHeight" value="10"/>
   </property>
</entity>
<entity name="Subscriber" class="ptolemy.actor.lib.Subscriber">
<doc>Produce a sequence generated by a matching Publisher.</doc>
   <property name="_icon" class="ptolemy.vergil.icon.BoxedValueIcon">
      <property name="attributeName" value="channel"/>
      <property name="displayWidth" value="60"/>
      <property name="boxColor" value="{0.76, 0.84, 0.6, 1.0}"/>
   </property>
</entity>
<entity name="SubscriptionAggregator" class="ptolemy.actor.lib.SubscriptionAggregator">
<doc>Aggregate sequences generated by matching Publishers.</doc>
   <property name="_icon" class="ptolemy.vergil.icon.BoxedValueIcon">
      <property name="attributeName" value="channel"/>
      <property name="displayWidth" value="60"/>
      <property name="boxColor" value="{1.0, 0.8, 0.0, 1.0}"/>
   </property>
</entity>
      </group>
    ?>
  </configure>
</entity>


