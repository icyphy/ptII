#!/bin/sh
# -[Mon Apr 12 23:03:39 2004 by cxh]-
# Return all the source files (with exceptions) in the ptII tree

# Filter out ptolemy/filter not moml/filter

/usr/bin/find . -type f -print | grep -v SCCS | grep -v shilman | grep -v devel | grep -v adm  | grep -v codeDoc | grep -v ptolemy/filter | grep -v com/microstar | grep -v CVS | grep -v collections | grep -v /cg/

