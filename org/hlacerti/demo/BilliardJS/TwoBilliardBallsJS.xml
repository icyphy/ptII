            <property name="script" class="ptolemy.actor.parameters.PortParameter" value="exports.setup = function() {&#10;	this.parameter('Vx');&#10;	this.parameter('Vy');&#10;	this.parameter('initY');&#10;	this.parameter('initX');&#10;	this.parameter('timeStep');&#10;	this.parameter('xMax');&#10;	this.parameter('yMax');&#10;	&#10;	this.output('x', {'type':'number'});&#10;	this.output('y', {'type':'number'});&#10;}&#10;exports.initialize = function() {&#10;	var self = this;&#10;	var x = this.getParameter('initX');&#10;	var y = this.getParameter('initY');&#10;	var Vx = this.getParameter('Vx');&#10;	var Vy = this.getParameter('Vy');&#10;	var xMax = this.getParameter('xMax');&#10;	var yMax = this.getParameter('yMax');&#10;	this.setInterval(function() {&#10;		this.send('x', x);&#10;		this.send('y', y);&#10;		x += Vx;&#10;		y += Vy;&#10;		if (Vx &gt; 0 &amp;&amp; x &gt;= xMax || Vx &lt; 0 &amp;&amp; x &lt;= 0) {&#10;			Vx = -Vx;&#10;		}&#10;		if (Vy &gt; 0 &amp;&amp; y &gt;= yMax || Vy &lt; 0 &amp;&amp; y &lt;= 0) {&#10;			Vy = -Vy;&#10;		}&#10;	}, this.getParameter('timeStep')*1000);&#10;}&#10;">
