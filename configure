	PTCOLT_JARS=$COLT_DIR/colt.jar$CLASSPATHSEPARATOR$PTCOLT_JAR
	# Put the ptcolt.jar second, after $COLT_DIR/colt.jar
        updateClasspath "$PTCOLT_JAR"
