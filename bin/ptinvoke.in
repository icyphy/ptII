PTHLACERTI_DIR=@PTHLACERTI_DIR@
PTJCERTI_JAR=@PTJCERTI_JAR@
if [ "$PTHLACERTI_DIR" = "hlacerti" ]; then 
    JCERTI_JARS="${CLASSPATHSEPARATOR}${PTJCERTI_JAR}"
fi

# Mac OS X 10.11 (El Capitan) ignores DYLD_LIBRARY_PATH unless sip is disabled.
if [ ! -z "$CERTI_HOME" ]; then
    if [ -d "$CERTI_HOME/lib" ]; then
        if [ ! -z "$DYLD_LIBRARY_PATH" ]; then
            export DYLD_LIBRARY_PATH=${CERTI_HOME}/lib:${DYLD_LIBRARY_PATH}
         else
            export DYLD_LIBRARY_PATH=${CERTI_HOME}/lib
        fi
        if [ ! -z "$LD_LIBRARY_PATH" ]; then
            export LD_LIBRARY_PATH=${CERTI_HOME}/lib:${LD_LIBRARY_PATH}
         else
            export LD_LIBRARY_PATH=${CERTI_HOME}/lib
        fi
    fi
fi


    
#### JNA - Java Native Access jar file (jna.jar).  See
