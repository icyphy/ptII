if [ "$PTJAVA3D_DIR" = "gr" ]; then 
    JAVA3D_JARS="${CLASSPATHSEPARATOR}${JAVA3D_JARS}"
    # Mac needs Jogl with Java 3D.
    PTJOGL_DIR=@PTJOGL_DIR@
    if [ ! -z "$PTJOGL_DIR" ]; then
        if [ "$DYLD_LIBRARY_PATH" != "" ]; then
            export DYLD_LIBRARY_PATH="${PTJOGL_DIR}/lib/natives/macosx-universal:${DYLD_LIBRARY_PATH}"
        else
            export DYLD_LIBRARY_PATH="${PTJOGL_DIR}/lib/natives/macosx-universal"
        fi
    fi
fi

