	if test "${PTJYTHON_BAT:-none}" != "none"; then
	    JYTHON_PROG_PARENT_DIR=`dirname "$PTJYTHON_BAT"`
   	    if test -f "$JYTHON_PROG_PARENT_DIR/jython.jar"; then
	        if test "$JYTHON_DIR" != "$JYTHON_PROG_PARENT_DIR"; then
