PTITEXTPDF_DIR=@PTITEXTPDF_DIR@
ITEXTPDF_JAR=""
if [ "$PTITEXTPDF_DIR" = "itextpdf" ]; then
    # directory that contains iText PDF interface,
    ITEXTPDF_JAR="${CLASSPATHSEPARATOR}@PTITEXTPDF_JAR@"
fi

#### Tini board stuff
